import sqlite3

rows = list()
conn = sqlite3.connect('/Users/niloo/Desktop/statistic_project/HiWeb.sqlite')
cur = conn.cursor()

# scraping signs from first link 
with open("MarketWatchInit.csv") as handle:
    info = handle.read()
infos = info.split('@')
for item in infos[2:-1]:
    rows.append(item.split(';'))

# saving in a table called signs
cur.execute('''CREATE TABLE signs 
            (i,izin_code,brief_name,complete_name,col5,initial,latest,
            last_trans,trans_num,trans_turnovers,trans_values,lowest,
            highest,yesterday,Eps,basis_turnovers,col17,col18,gp_code,allowed_hp,
            allowed_lp,all_trans_nums,col23)''')
for items in rows:
    for row in items:
        splitted_row = row.split(',')
        if len(splitted_row) == 8:
            for i in range(15):
                splitted_row.append('')
        cur.execute('''INSERT INTO signs VALUES(?,?,?,?,?,?,?,?,?,?,?
                   ,?,?,?,?,?,?,?,?,?,?,?,?)''',tuple(splitted_row))
    conn.commit()

# scraping date of transactions from second link and saving in the hiweb table
with open ('InstCalendar.csv') as handle:
    file = handle.read()
cur.execute('''CREATE TABLE hiweb (i bigint,s_date date,m_date date,
last_price integer,trans_turnovers integer)''')
rows = file.split(';')
for row in rows:
    items = row.split(',')
    cur.execute('''INSERT INTO hiweb VALUES(43362635835198978,?,?,?,?)''',tuple(items))
conn.commit()



